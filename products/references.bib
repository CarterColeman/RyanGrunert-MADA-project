	keywords = {Avian malaria,Florida,Haemoproteus plataleae,Pelicaniformes,Plasmodium,Wading birds,White Ibis,haemosporidia},
	number = {3},
	pages = {220--225},
	publisher = {Elsevier Ltd},
	title = {{Diversity and prevalence of hemoparasites of wading birds in southern Florida, USA}},
